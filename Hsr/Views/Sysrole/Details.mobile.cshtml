<div class="ui-body ui-body-a ui-corner-all">
<fieldset>
    <legend>Sysrole</legend>

    <div class="display-label">@Html.LabelFor(model => model.RoleName)</div>
    <div class="display-field">@Model.RoleName</div>

    <div class="display-label">@Html.LabelFor(model => model.Category)</div>
    <div class="display-field">@Model.Category</div>

    <div class="display-label">@Html.LabelFor(model => model.Isenabled)</div>
